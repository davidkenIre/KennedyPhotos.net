@model IEnumerable<Photos.Models.Album>


    <div class="row">
        <div class="col-md-4">
            <h2>Cameras</h2>
            <ol>
                <li><a href="http://lattuce.duckdns.org:171/">Realtime Front</a></li>
                <li><a href="http://lattuce.duckdns.org:170/">Realtime Back</a></li>
                <li><a href="http://lattuce.duckdns.org:5648">Realtime Both</a></li>

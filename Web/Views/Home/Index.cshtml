            </ol>
        </div>
        <div class="col-md-4">
            <h2>Recent Albums</h2>
            <ol>
                @foreach (var item in Model)
            {
                    <li><a href="/Photos/ViewAlbum/@item.Id">@item.AlbumName</a></li>
                }

            </ol>
            <p><a class="btn btn-default" href="/Photos/Index" role="button">See All Albums &raquo;</a></p>
        </div>
        <div class="col-md-4">
            <h2>Heading</h2>
